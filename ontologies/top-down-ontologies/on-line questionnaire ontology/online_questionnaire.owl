    <owl:NamedIndividual rdf:about="http://www.glamurs.eu/ontologies/2016/TBOX/top-down/questionnaire#term:model">
        <rdf:type rdf:resource="http://www.glamurs.eu/ontologies/2016/TBOX/top-down/top-down-common#Term"/>
