        <top-down-common:partOfDiscipline rdf:resource="http://www.glamurs.eu/ontologies/2016/TBOX/top-down/top-down-common#discipline:ecologicalEconomics"/>
        <top-down-common:partOfDiscipline rdf:resource="http://www.glamurs.eu/ontologies/2016/TBOX/top-down/top-down-common#discipline:happinessResearch"/>
