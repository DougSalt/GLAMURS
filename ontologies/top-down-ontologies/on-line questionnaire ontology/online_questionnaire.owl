    <!-- http://www.glamurs.eu/ontologies/2016/TBOX/top-down/questionnaire#term:socialPractice -->

    <owl:NamedIndividual rdf:about="http://www.glamurs.eu/ontologies/2016/TBOX/top-down/questionnaire#term:socialPractice">
